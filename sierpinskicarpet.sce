a = 3;function y = f1(x)    y = [x(1);x(2)]/a;    endfunctionfunction y = f2(x)    y = [1+x(1);x(2)]/a;    endfunctionfunction y = f3(x)    y = [x(1);1+x(2)]/a;    endfunctionfunction y = f4(x)    y = [1+x(1);1+x(2)]/a;    endfunctionx = [0;0];n = 500000;X = zeros(2,n/5);//# of iterations- n should be divisible by 5!!!!!!for i = 1:n    r = rand(1,1);    if r < 1/4 then        x = f1(x);    elseif 1/4 <= r & r < 1/2          x = f2(x);    elseif 1/2 <= r & r < 3/4        x = f3(x);    else        x = f4(x);    end    if i > n-n/5 then        X(:,i-(n-n/5)) = x;    endendplot(X(1,:),X(2,:),'.','MarkerSize',1.5);xtitle('Computer Experiment 4.2: Sierpinski Carpet');