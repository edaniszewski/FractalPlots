function y = f1(x)    y = [x(1);x(2)]/2;    endfunctionfunction y = f2(x)    y = [1+x(1);x(2)]/2;    endfunctionfunction y = f3(x)    y = [x(1);1+x(2)]/2;    endfunctionx = [0;0];n = 500000;  //# of iterations- n should be divisible by 5!!!!!!X = zeros(2,n/5);for i = 1:n    r = rand(1,1);    if r < 1/3 then        x = f1(x);    elseif 1/3 <= r & r < 2/3          x = f2(x);    else        x = f3(x);    end    if i > n-n/5 then        X(:,i-(n-n/5)) = x;    endendplot(X(1,:),X(2,:),'.','MarkerSize',2);xtitle('Computer Experiment 4.2: Sierpinski Gasket');